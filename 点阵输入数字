#include <stdio.h>
int main()
{
	char a[7][70]={

'*','*','*','*','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ',

'*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',

'*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',

'*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ',

'*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',

'*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ',' ',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*',' ',' ',' ','*',' ',' ',' ',' ',' ',' ','*',' ',' ',

'*','*','*','*','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' ',' ',' ',' ',' ','*',' ',' ','*','*','*','*','*',' ',' ','*','*','*','*','*',' ',' '

};

    int n;
    scanf("%d",&n);
    int i,num;
    for( i=0; i<n; i++ ){
    	scanf("%d",&num);
    	printf("%d:\n",num);
    	
    	int t=num;
    	int j=0,d[10];
    	while(t!=0 ){
    		d[j]=t%10;
    		j++;
    		t=t/10;
    	}
    int p=j,k,m;
    for( m=0; m<7; m++ ){
    	for( j--; j>=0; j-- ){
    		for( k=0; k<7; k++ ){
    			printf("%c",a[m][d[j]*7+k]);
    		}
    	}
    	printf("\n");
    	j=p;
    }
		
}
    return 0;
}
