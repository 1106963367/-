#include <stdio.h>
#include <math.h>
int main()
{
	int n;
	int i,num;
	double a;
	scanf("%d",&n);
	for( i=0; i<n; i++ ){
		scanf("%d",&num);
		if( num>10000 || num<-10000){
			i--;
			continue;
		}
		if( num<0 ){
			printf("ERROR\n");
			continue;
		}else
		a=sqrt(num);
		printf("%.2f\n",a);
	}
	
	return 0;
}
